option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "eeflow.settings"
    "PYTHONPATH": "/opt/python/current/app/eeflow:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: eeflow/eeflow/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
